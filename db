CREATE TABLE `default` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `language_title` text,
  `choose_language` text,
  `browse_helpers` text,
  `browse_employers` text,
  `helper_login_signup` text,
  `employer_login_signup` text,
  `fetching_language` text,
  `footer_text` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


CREATE TABLE `innvitations_temp_table` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sender_id` int(11) NOT NULL,
  `receiver_id` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


CREATE TABLE `invitation` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sender_id` text,
  `email` text,
  `vouchar` text,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


CREATE TABLE `main_languages` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `language_title` text NOT NULL,
  `sorting` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

CREATE TABLE `messages` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sender_id` text,
  `receiver_id` text,
  `message` text,
  `date` text,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


CREATE TABLE `profile` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` text,
  `nationality` text,
  `religion` text,
  `country` int(11) DEFAULT '0',
  `city` text,
  `photo1` text,
  `photo2` text,
  `salary` int(11) DEFAULT '0',
  `salary_type` int(11) NOT NULL DEFAULT '0',
  `salary_currency` int(11) NOT NULL DEFAULT '-1',
  `job_type` int(11) NOT NULL DEFAULT '0',
  `distance` text,
  `accomodation` int(11) DEFAULT '0',
  `home_type` int(11) DEFAULT '0',
  `work_days` text,
  `profession` text,
  `responsibilities` text,
  `availability` text,
  `contract_situation` text,
  `years_of_experience` text,
  `cooking` text,
  `gender` int(11) DEFAULT '0',
  `needed_nationality` text,
  `age` text,
  `needed_religion` text,
  `marital_status` text,
  `education` int(11) DEFAULT '0',
  `languages` text,
  `photo1_aprooved` int(11) NOT NULL DEFAULT '0',
  `photo2_aprooved` int(11) NOT NULL DEFAULT '0',
  `app_language` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

CREATE TABLE `recommendations` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sender_id` text,
  `receiver_id` text,
  `recommendation` text,
  `date` text,
  `approved` text,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


CREATE TABLE `spoken_languages` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `language` text,
  `sorting` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


CREATE TABLE `subscription_settings` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `employers_subscribe_contacts` int(11) NOT NULL,
  `employers_subscribe_days` int(11) NOT NULL,
  `minimum_points_to_redeem` int(11) NOT NULL,
  `won_contacts` int(11) NOT NULL,
  `won_days` int(11) NOT NULL,
  `helpers_subscribe_contacts` int(11) NOT NULL,
  `helpers_subscribe_days` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


CREATE TABLE `test` (
  `OOI` text NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


CREATE TABLE `transactions` (
  `id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL,
  `transaction_id` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

CREATE TABLE `users` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `first_name` text,
  `last_name` text,
  `email` text,
  `password` text,
  `voucher` text,
  `first_time_profile_edit` int(11) DEFAULT NULL,
  `last_seen` int(11) DEFAULT NULL,
  `user_type` int(11) DEFAULT NULL,
  `account_type` int(11) DEFAULT NULL,
  `expiry_date` int(11) DEFAULT NULL,
  `remaining_contacts` int(11) DEFAULT NULL,
  `earned_points` int(11) DEFAULT NULL,
  `subscribe_to_newsletter` text,
  `longitude_value` text,
  `latitude_value` text,
  `verified` int(11) DEFAULT NULL,
  `token` text NOT NULL,
  `active` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

CREATE TABLE `view1` (
   `id` INT(11) NOT NULL DEFAULT '0',
   `first_name` TEXT NULL DEFAULT NULL,
   `last_name` TEXT NULL DEFAULT NULL,
   `email` TEXT NULL DEFAULT NULL,
   `password` TEXT NULL DEFAULT NULL,
   `voucher` TEXT NULL DEFAULT NULL,
   `first_time_profile_edit` INT(11) NULL DEFAULT NULL,
   `last_seen` INT(11) NULL DEFAULT NULL,
   `user_type` INT(11) NULL DEFAULT NULL,
   `account_type` INT(11) NULL DEFAULT NULL,
   `expiry_date` INT(11) NULL DEFAULT NULL,
   `remaining_contacts` INT(11) NULL DEFAULT NULL,
   `earned_points` INT(11) NULL DEFAULT NULL,
   `subscribe_to_newsletter` TEXT NULL DEFAULT NULL,
   `longitude_value` TEXT NULL DEFAULT NULL,
   `latitude_value` TEXT NULL DEFAULT NULL,
   `verified` INT(11) NULL DEFAULT NULL,
   `token` TEXT NOT NULL,
   `active` INT(11) NOT NULL DEFAULT '1'
) ENGINE=MyISAM;

