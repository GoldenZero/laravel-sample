CREATE TABLE `archived_messages` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sender_id` text,
  `receiver_id` text,
  `message` text,
  `date` text,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

